table.table.table-striped.table-hover
  thead
    tr
      th Titre
      th URL
      th Nom d'utilisateur
      th Mot de passe 
      th Categorie
  tbody
        
    for doc in docs
     tr
      td #{doc.title}
      td #{doc.url} 
      td #{doc.passname} 
      td #{doc.password} 
      td #{doc.category}
      td 
        a(href="/update/#{doc._id}") Modifier 
      td 
        a(href="/delete/#{doc._id}") Supprimer
            
